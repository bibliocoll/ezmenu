{
  "name": "ezmenu",
  "version": "0.1.0",
  "description": "EZMenu is a jQuery.mmenu-based UI for OCLC's EZProxy",
  "main": "frontend/js/ezmenu/main.js",
  "directories": {
    "baseURL": ".",
    "packages": "./jspm_packages"
  },
  "dependencies": {},
  "devDependencies": {
    "htmlsrv": "^1.2.1",
    "jspm": "^0.16.19",
    "node-sass": "^3.4.2"
  },
  "scripts": {
    "postinstall": "jspm install",
    "start": "node-sass -w frontend/sass/loggedin/menu.sass -o demo/ & SRV=demo htmlsrv",
    "build": "tooling/build.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bibliocoll/ezmenu.git"
  },
  "keywords": [
    "EZProxy",
    "jQuery",
    "mmenu",
    "jQuery.mmenu"
  ],
  "author": "biblio@coll.mpg.de",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/bibliocoll/ezmenu/issues"
  },
  "homepage": "https://github.com/bibliocoll/ezmenu#readme",
  "jspm": {
    "directories": {},
    "configFile": "tooling/config.js",
    "dependencies": {
      "BeSite/jQuery.mmenu": "github:FrDH/jQuery.mmenu@^5.5.3",
      "domready": "npm:domready@^1.0.8",
      "es6-promise": "npm:es6-promise@^3.0.2",
      "jquery": "github:components/jquery@^2.1.4",
      "whatwg-fetch": "npm:whatwg-fetch@^0.10.1"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "capaj/systemjs-hot-reloader": "github:capaj/systemjs-hot-reloader@^0.5.1",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
